    "import pandas as pd\n",
    "df=pd.DataFrame({'data':['10/2/2011','11/2/2011','12/2/2011','13/12/2011'],\n",
    "                  'Event':['music','poetry','theatre','comedy'],\n",
    "                  'cost':[10000,5000,15000,2000]})\n",
    "df\n",
    "df['Discount_price']=df.apply(lambda a:a.cost-(a.cost*0.1),axis=1)\n",
    "df"
